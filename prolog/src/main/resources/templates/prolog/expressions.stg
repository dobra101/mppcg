binaryExpression(lhs, rhs, operator, exprCount, math) ::= <<
Expr_<exprCount> <if(math)>is<else>=<endif> <lhs> <operator> <rhs>
>>

identifierExpression(name, stateCount, exprCount) ::= <<
get(State_<stateCount>, '<name>', Expr_<exprCount>)
>>

