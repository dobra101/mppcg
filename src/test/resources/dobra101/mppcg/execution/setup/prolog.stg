setup(name, execution, result) ::= <<
start(S0),
<if(execution)><execution :{ it | trans('<it.method>', S<i0>, S<i>)}; separator=",\n">,<endif>
Result = S<length(execution)>,
<result :{ it | out(Result, <it.call>, Result_<it.name>), !}; separator=",\n">.
>>

//<chain :{ it | <if(it.parameterized)>'<it.methodName>'(X=<it.parameter>)<else><it.method>}; separator="->">
methodChain(chain) ::= <<
<chain :{ it | <if(it.parameterized)>'<it.methodName>'(X=<it.parameter>)<else>'<it.methodName>'<endif>}; separator="->">
>>